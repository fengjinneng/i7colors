                android:textColor="@color/erjibiaoti"
                android:textSize="@dimen/x24"
                app:layout_constraintStart_toStartOf="@+id/activity_qiugouxiangqing_fenlei"
                app:layout_constraintTop_toTopOf="@+id/textView42" />

            <TextView
                android:id="@+id/activity_qiugouxiangqing_shuliang"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="100"
                android:textColor="@color/erjibiaoti"
                android:textSize="@dimen/x24"
                app:layout_constraintStart_toStartOf="@+id/activity_qiugouxiangqing_baozhuang"
                app:layout_constraintTop_toTopOf="@+id/textView43" />

            <TextView
                android:id="@+id/activity_qiugouxiangqing_jiaohuoshijian"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="2018-08-04"
                android:textColor="@color/erjibiaoti"
                android:textSize="@dimen/x24"
                app:layout_constraintStart_toStartOf="@+id/activity_qiugouxiangqing_shuliang"
                app:layout_constraintTop_toTopOf="@+id/textView44" />

            <TextView
                android:id="@+id/activity_qiugouxiangqing_fukuanfangshi"
                android:layout_width="wrap_content"
