<?xml version="1.0" encoding="utf-8"?>
    <android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/fengexian"
        tools:context=".ui.activity.qiugoudating.QiugouxiangqingActivity">



        <TextView
            android:id="@+id/activity_qiugouxiangqing_daojishi"
            android:layout_width="@dimen/x160"
            android:layout_height="@dimen/x100"
            android:background="@drawable/background_shuruwenbenkuang"
            android:gravity="center"
            android:text="19天18小时"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/include7" />

    <TextView
        android:id="@+id/activity_qiugouxiangqing_shangpinming"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/baise"
        android:gravity="center_vertical"
        android:paddingLeft="16dp"
        android:text="防紫外线整理剂"
        android:textColor="@color/biaoti"
        android:textSize="@dimen/x30"
        app:layout_constraintBottom_toBottomOf="@+id/activity_qiugouxiangqing_daojishi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/activity_qiugouxiangqing_daojishi"
        app:layout_constraintTop_toTopOf="@+id/activity_qiugouxiangqing_daojishi" />

        <View
