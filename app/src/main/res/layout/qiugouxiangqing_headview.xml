                android:textColor="@color/erjibiaoti"
                android:textSize="@dimen/x24"
                app:layout_constraintStart_toStartOf="@+id/activity_qiugouxiangqing_diqu"
                app:layout_constraintTop_toTopOf="@+id/textView48" />


