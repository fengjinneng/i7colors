<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">


    <TextView
        android:background="@drawable/background_dengluwenbenkuang_quexia"
        android:id="@+id/baidu"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x90"
        android:text="百度地图"
        android:gravity="center"
        android:textColor="@color/erjibiaoti"
        />


    <TextView
        android:id="@+id/gaode"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x90"
        android:text="高德地图"
        android:background="@drawable/background_dengluwenbenkuang"
        android:gravity="center"
        android:textColor="@color/erjibiaoti"
        />

    <TextView
        android:id="@+id/tengxun"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x90"
        android:text="腾讯地图"
        android:background="@drawable/background_dengluwenbenkuang_queshang"
        android:gravity="center"
        android:textColor="@color/erjibiaoti"
        />
    <View android:layout_width="match_parent"
        android:background="@color/trans"
        android:layout_height="@dimen/x60" />

</LinearLayout>
