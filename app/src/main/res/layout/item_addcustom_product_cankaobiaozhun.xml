<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/item_wodehuodan_cankaobianzhun_text"
        android:layout_width="0dp"
        android:layout_height="@dimen/x64"
        android:background="@drawable/background_wenbenshurubiankuang"
        android:gravity="center"
        android:maxLines="2"
        android:ellipsize="end"

        app:layout_constraintEnd_toStartOf="@+id/item_wodehuodan_cankaobianzhun_reduce"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/item_wodehuodan_cankaobianzhun_reduce"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/cankaobiaozhun_reduce" />

    <TextView
        android:id="@+id/textView231"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/x20"
        android:layout_marginStart="8dp"
        app:layout_constraintStart_toStartOf="@+id/item_wodehuodan_cankaobianzhun_text"
        app:layout_constraintTop_toBottomOf="@+id/item_wodehuodan_cankaobianzhun_text" />
</android.support.constraint.ConstraintLayout>