        app:layout_constraintTop_toBottomOf="@+id/activity_message_dianzan_layout" />

    <android.support.constraint.ConstraintLayout
        android:id="@+id/activity_message_wode_layout"
        android:layout_width="0dp"
        android:background="@color/baise"
