<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/baise"
    tools:context=".ui.activity.tuangou.WoyaotuangouActivity">

    <include
        android:id="@+id/include17"
        layout="@layout/toolbar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraintLayout12"
        android:layout_width="344dp"
        android:layout_height="@dimen/x84"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/biankuang_canyubaojia_tishi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/include17">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_marginStart="16dp"

            android:layout_marginTop="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@mipmap/fabuqiugou_you" />

        <TextView
            android:id="@+id/textView82"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp"
            android:gravity="center_vertical"
            android:text="提交成功后系统将自动帮您生成唯一的“团购编号”，可在“参 与团购记录详细”中查看！"
            android:textSize="@dimen/x24"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView4"
            app:layout_constraintTop_toTopOf="parent" />
    </android.support.constraint.ConstraintLayout>

    <TextView
        android:id="@+id/textView95"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/x40"
        android:drawableLeft="@mipmap/bitiantupian"
        android:text="我的认领量:"
        android:textSize="@dimen/x28"
        app:layout_constraintStart_toStartOf="@+id/constraintLayout12"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout12" />

    <TextView
        android:id="@+id/textView106"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/x68"
        android:drawableLeft="@mipmap/bitiantupian"
        android:text="联系人:"
        android:textSize="@dimen/x28"
        app:layout_constraintStart_toStartOf="@+id/textView95"
        app:layout_constraintTop_toBottomOf="@+id/textView95" />

    <TextView
        android:id="@+id/textView108"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/x68"
        android:drawableLeft="@mipmap/bitiantupian"
        android:text="联系人方式:"
        android:textSize="@dimen/x28"
        app:layout_constraintStart_toStartOf="@+id/textView106"
        app:layout_constraintTop_toBottomOf="@+id/textView106" />

    <TextView
        android:id="@+id/textView113"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/x68"
        android:drawableLeft="@mipmap/bitiantupian"
        android:text="公司名称:"
        android:textSize="@dimen/x28"
        app:layout_constraintStart_toStartOf="@+id/textView108"
        app:layout_constraintTop_toBottomOf="@+id/textView108" />

    <TextView
        android:id="@+id/textView115"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/x68"
        android:drawableLeft="@mipmap/bitiantupian"
        android:text="公司所属区域:"
        android:textSize="@dimen/x28"
        app:layout_constraintStart_toStartOf="@+id/textView113"
        app:layout_constraintTop_toBottomOf="@+id/textView113" />

    <TextView
        android:id="@+id/textView119"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/x68"
        android:drawableLeft="@mipmap/bitiantupian"
        android:text="公司详细地址:"
        android:textSize="@dimen/x28"
        app:layout_constraintStart_toStartOf="@+id/textView115"
        app:layout_constraintTop_toBottomOf="@+id/textView115" />

    <EditText
        android:id="@+id/activity_woyaotuangou_renlingliang"
        android:paddingLeft="8dp"
        android:layout_width="@dimen/x228"
        android:layout_height="@dimen/x64"
        android:layout_marginStart="24dp"
        android:background="@drawable/background_shuruwenbenkuang"
        android:inputType="numberDecimal"
        android:textSize="@dimen/x24"
        android:hint="填写数字"
        app:layout_constraintBottom_toBottomOf="@+id/textView95"
        app:layout_constraintStart_toEndOf="@+id/textView115"
        app:layout_constraintTop_toTopOf="@+id/textView95" />

    <EditText
        android:id="@+id/activity_woyaotuangou_lianxiren"
        android:layout_width="0dp"
        android:layout_height="@dimen/x64"
        android:layout_marginEnd="8dp"
        android:background="@drawable/background_shuruwenbenkuang"
        android:hint="请输入联系人"
        android:paddingLeft="8dp"
        android:inputType="textPersonName"
        android:textSize="@dimen/x24"
        app:layout_constraintBottom_toBottomOf="@+id/textView106"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/activity_woyaotuangou_renlingliang"
        app:layout_constraintTop_toTopOf="@+id/textView106" />

    <EditText
        android:id="@+id/activity_woyaotuangou_phone"
        android:layout_width="0dp"
        android:layout_height="@dimen/x64"
        android:layout_marginEnd="8dp"
        android:background="@drawable/background_shuruwenbenkuang"
        android:hint="请输入联系方式"
        android:paddingLeft="8dp"
        android:inputType="phone"
        android:textSize="@dimen/x24"
        app:layout_constraintBottom_toBottomOf="@+id/textView108"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/activity_woyaotuangou_lianxiren"
        app:layout_constraintTop_toTopOf="@+id/textView108" />

    <EditText
        android:id="@+id/activity_woyaotuangou_companyname"
        android:layout_width="0dp"
        android:layout_height="@dimen/x64"
        android:layout_marginEnd="8dp"
        android:background="@drawable/background_shuruwenbenkuang"
        android:hint="请输入公司名称"
        android:paddingLeft="8dp"
        android:inputType="textPersonName"
        android:textSize="@dimen/x24"
        app:layout_constraintBottom_toBottomOf="@+id/textView113"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/activity_woyaotuangou_phone"
        app:layout_constraintTop_toTopOf="@+id/textView113" />

    <EditText
        android:id="@+id/activity_woyaotuangou_company_address"
        android:layout_width="0dp"
        android:layout_height="@dimen/x64"
        android:layout_marginEnd="8dp"
        android:paddingLeft="8dp"
        android:background="@drawable/background_shuruwenbenkuang"
        android:hint="请输入公司详细地址"
        android:inputType="textPersonName"
        android:textSize="@dimen/x24"
        app:layout_constraintBottom_toBottomOf="@+id/textView119"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/activity_woyaotuangou_companyname"
        app:layout_constraintTop_toTopOf="@+id/textView119" />

    <TextView
        android:id="@+id/activity_woyaotuangou_submit"
        android:layout_width="0dp"
        android:layout_height="@dimen/x98"
        android:text="提交"
        android:textSize="@dimen/x30"
        android:textColor="@color/baise"
        android:gravity="center"
        android:background="@mipmap/anniujianbian"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/activity_woyaotuangou_cancel"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/activity_woyaotuangou_cancel"
        android:layout_width="0dp"
        android:layout_height="@dimen/x98"
        android:text="取消"
        android:textSize="@dimen/x30"
        android:background="@color/fengexian"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/activity_woyaotuangou_submit" />

    <TextView
        android:id="@+id/activity_woyaotuangou_fanwei"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="0.5吨\u003C认领量\u003C1吨 "
        android:textColor="@color/chunhongse"
        android:textSize="@dimen/x20"
        app:layout_constraintBottom_toBottomOf="@+id/activity_woyaotuangou_renlingliang"
        app:layout_constraintStart_toEndOf="@+id/textView132"
        app:layout_constraintTop_toTopOf="@+id/activity_woyaotuangou_renlingliang"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/activity_woyaotuangou_choiceAddress"
        android:layout_width="@dimen/x228"
        android:layout_height="@dimen/x64"
        android:text="请选择"
        android:textSize="@dimen/x24"
        android:gravity="center"
        android:background="@drawable/background_wenbenshurubiankuang"
        android:paddingRight="8dp"
        android:drawableRight="@mipmap/xialajiantou"
        app:layout_constraintBottom_toBottomOf="@+id/textView115"
        app:layout_constraintStart_toStartOf="@+id/activity_woyaotuangou_companyname"
        app:layout_constraintTop_toTopOf="@+id/textView115" />

    <TextView
        android:id="@+id/textView132"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="吨"
        android:textSize="@dimen/x24"
        app:layout_constraintBottom_toBottomOf="@+id/activity_woyaotuangou_renlingliang"
        app:layout_constraintStart_toEndOf="@+id/activity_woyaotuangou_renlingliang"
        app:layout_constraintTop_toTopOf="@+id/activity_woyaotuangou_renlingliang" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/x68"
        android:textSize="@dimen/x28"
        android:text="英雄码:"
        app:layout_constraintStart_toStartOf="@+id/textView119"
        app:layout_constraintTop_toBottomOf="@+id/textView119" />

    <EditText
        android:id="@+id/activity_woyaotuangou_yinxiongma"
        android:layout_width="0dp"
        android:layout_height="@dimen/x64"
        android:background="@drawable/background_shuruwenbenkuang"
        android:hint="推荐人英雄码"
        android:inputType="textPersonName"
        android:paddingLeft="8dp"
        android:textSize="@dimen/x24"
        app:layout_constraintBottom_toBottomOf="@+id/textView11"
        app:layout_constraintEnd_toEndOf="@+id/activity_woyaotuangou_choiceAddress"
        app:layout_constraintStart_toStartOf="@+id/activity_woyaotuangou_company_address"
        app:layout_constraintTop_toTopOf="@+id/textView11" />

    <TextView
        android:id="@+id/activity_woyaotuangou_yinxiongma_shuoming"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:text="查看使用说明"
        android:textSize="@dimen/x24"
        android:textColor="@color/chunhongse"
        app:layout_constraintBottom_toBottomOf="@+id/activity_woyaotuangou_yinxiongma"
        app:layout_constraintStart_toEndOf="@+id/activity_woyaotuangou_yinxiongma"
        app:layout_constraintTop_toTopOf="@+id/activity_woyaotuangou_yinxiongma" />

    <TextView
        android:id="@+id/textView130"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/x68"
        android:textSize="@dimen/x28"
        android:text="是否需要样品:"
        android:drawableLeft="@mipmap/bitiantupian"
        app:layout_constraintStart_toStartOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <RadioGroup
        android:id="@+id/activity_woyaotuangou_yangpin_group"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="@+id/textView130"
        app:layout_constraintStart_toStartOf="@+id/activity_woyaotuangou_yinxiongma"
        app:layout_constraintTop_toTopOf="@+id/textView130">

        <RadioButton
            android:id="@+id/activity_woyaotuangou_radiobutton1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="true"
            android:text="是" />

        <RadioButton
            android:id="@+id/activity_woyaotuangou_radiobutton2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="否" />

    </RadioGroup>


</android.support.constraint.ConstraintLayout>